{
    "name": "nazrulalif/laravel-pem-license-validator",
    "description": "Offline PEM license validation for Laravel applications using RSA-4096 signatures",
    "type": "library",
    "keywords": [
        "laravel",
        "license",
        "validation",
        "pem",
        "rsa",
        "offline"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nazrul",
            "email": "nazrulism17@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nazrulalif\\LaravelPemLicenseValidator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nazrulalif\\LaravelPemLicenseValidator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nazrulalif\\LaravelPemLicenseValidator\\LicenseServiceProvider"
            ],
            "aliases": {
                "License": "Nazrulalif\\LaravelPemLicenseValidator\\Facades\\License"
            }
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nazrulalif/laravel-pem-license-validator"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}